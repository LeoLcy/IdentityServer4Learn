
@{
    ViewData["Title"] = "Index";
}
@using Microsoft.AspNetCore.Authentication
<a href="Home/Logout">退出</a>
<h2>Claims</h2>

<dl>
    这是测试
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }


</dl>

